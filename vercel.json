{
  "version": 2,
  "functions": {
    "backend/server.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/backend/server.js"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/flutter_app/build/web/$1"
    }
  ]
}
